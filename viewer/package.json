{
  "name": "poe-dat-studio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.vue --fix",
    "format": "prettier --write \"src/**/*.{ts,vue,css,json}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "cd .. && husky viewer/.husky"
  },
  "dependencies": {
    "@vueuse/core": "^11.3.0",
    "@vscode/codicons": "^0.0.36",
    "comlink": "^4.4.2",
    "evt": "^2.5.9",
    "expiry-map": "^2.0.0",
    "file-saver": "^2.0.5",
    "idb": "^8.0.3",
    "lucide-vue-next": "^0.468.0",
    "neverthrow": "^8.2.0",
    "pathofexile-dat": "^14.0.1",
    "pathofexile-dat-schema": "^8.0.0",
    "pinia": "^2.3.0",
    "string-metric": "^0.3.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.7",
    "@types/file-saver": "^2.0.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^10.0.0",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
